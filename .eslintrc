{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "document": true,
    "navigator": true,
    "window": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "react/prefer-stateless-function": 0,
    "no-extra-boolean-cast": 0,
    "class-methods-use-this": "off",
    "semi": 0,
    "react/jsx-props-no-spreading": 0,
    "react/no-array-index-key":0
  }
}